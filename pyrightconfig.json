{
  "$schema": "https://raw.githubusercontent.com/microsoft/pyright/main/packages/pyright/schema/pyrightconfig.schema.json",
  "venvPath": "backend",
  "venv": "venv",
  "pythonVersion": "3.11",
  "typeCheckingMode": "basic",
  "executionEnvironments": [
    {
      "root": "./backend",
      "extraPaths": ["./backend"]
    }
  ],
  "exclude": [
    "frontend",
    "deployment",
    "node_modules",
    "**/__pycache__",
    ".git",
    "backend/venv",
    "backend/outputs",
    "backend/uploads",
    "**/.venv",
    "**/.pytest_cache",
    "**/.next",
    "**/dist",
    "**/build",
    "**/.turbo",
    ".cursor",
    ".vercel"
  ]
}


